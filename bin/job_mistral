#!/bin/bash
#SBATCH --job-name=fesom2.0
#SBATCH -p compute2
#SBATCH --ntasks=288
#SBATCH --time=06:00:00
#SBATCH -o slurm-out.out
#SBATCH -e slurm-err.out
#SBATCH -Aab0246 
#SBATCH --mail-type=FAIL
#SBATCH --mail-user=jstreffi@awi.de

set -x

ulimit -s unlimited

# determine JOBID
JOBID=`echo $SLURM_JOB_ID |cut -d"." -f1`

date
srun --mpi=pmi2 --ntasks=288 ./fvom.x > "fesom.out"
date
