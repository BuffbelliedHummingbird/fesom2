#!/bin/bash
#SBATCH --job-name=fesom2.0
#SBATCH -p compute2,compute
#SBATCH --ntasks=288
#SBATCH --time=06:00:00
#SBATCH -o slurm-out.out
#SBATCH -e slurm-err.out
#SBATCH -Aab0995

set -x

#SBATCH --qos=express


ulimit -s unlimited

# determine JOBID
JOBID=`echo $SLURM_JOB_ID |cut -d"." -f1`

date
srun --mpi=pmi2 --ntasks=288 ../build/src/fesom > "fesom.out"
date
