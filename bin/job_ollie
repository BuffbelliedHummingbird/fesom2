#!/bin/bash
#SBATCH --job-name=fesom2.0_ALE
#SBATCH -p mpp
#SBATCH --ntasks=72
#SBATCH --time=0:30:00
#SBATCH -o slurm-out.out
#SBATCH -e slurm-err.out

set -x

ulimit -s unlimited

module purge
module load intel.compiler
module load intel.mpi
module load netcdf

# determine JOBID
JOBID=$SLURM_JOB_ID 

date
srun ./fvom.x > "fesom.out"
date
